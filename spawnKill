#KILL PROCESSES FUNCTION 
function cleanup {
	ps -e | grep APM | sed 's/\|/ /' | awk '{print $1}' > ps.txt
	File="ps.txt"
	procList = $(cat ps.txt)
	For Line in $Lines 
	do
		kill $line 
	done
}
	
